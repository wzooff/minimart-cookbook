cookbooks:
<% @cookbook_list.each do |item, location, tags| %>
  <%= item %>:
    git:
      location: '<%= location %>'
      <% if tags == 'master' %>
      branch: "<%= tags %>"
      <% else %>
      tags:
        <% tags.each do |tag| %>
        - <%= tag %>
        <% end %>
      <% end %>
<% end %>
